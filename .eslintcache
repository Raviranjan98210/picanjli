\n","/mnt/FE4C191E4C18D2EF/Developement/React/pixabay-image-tailwind/src/components/ui/DetailCard/DetailCard.js",[],"/mnt/FE4C191E4C18D2EF/Developement/React/pixabay-image-tailwind/src/components/ui/DetailCard/CardContent.js",[],"/mnt/FE4C191E4C18D2EF/Developement/React/pixabay-image-tailwind/src/pages/DetailAndDownload/DetailAndDownload.js",[],"/mnt/FE4C191E4C18D2EF/Developement/React/pixabay-image-tailwind/src/components/ui/SkeltonLoading/SkeltonLoading.js",["142"],"import React from \"react\";\nimport Grid from \"@material-ui/core/Grid\";\nimport Box from \"@material-ui/core/Box\";\nimport Skeleton from \"@material-ui/lab/Skeleton\";\n\nfunction SkeltonLoading({ noOfBoxes }) {\n  return (\n    <>\n      {Array.from(new Array(noOfBoxes)).map((item, index) => (\n        <Box key={index} width={300} my={5} className=\"mx-auto max-w-full\">\n          <Skeleton variant=\"rect\" width={300} height={200} />\n\n          <Skeleton />\n          <Skeleton width=\"60%\" />\n        </Box>\n      ))}\n    </>\n  );\n}\n\nSkeltonLoading.defaultProps = {\n  noOfBoxes: 8,\n};\nexport default SkeltonLoading;\n","/mnt/FE4C191E4C18D2EF/Developement/React/pixabay-image-tailwind/src/pages/Search/Search.js",["143"],"import React, { useState, useEffect } from \"react\";\nimport { useParams } from \"react-router-dom\";\n\nimport api from \"../../api/index\";\nimport { GlobalStateValue } from \"../../context/GlobalState\";\nimport {\n  SET_SEARCHED_IMAGES,\n  RESET_SEARCHED_IMAGES,\n} from \"../../context/Types\";\nimport CustomInfiniteScroll from \"../../components/ui/InfiniteScroll/InfinteScroll\";\nimport NoImagesFound from \"../../components/ui/NoImagesFound/NoImagesFound\";\nimport ProgressBar from \"../../components/ui/ProgressBar/ProgressBar\";\nimport SkeltonLoading from \"../../components/ui/SkeltonLoading/SkeltonLoading\";\n\nconst Search = () => {\n  const { searchTerm } = useParams();\n  const [{ searchedImages }, dispatch] = GlobalStateValue();\n  const [page, setPage] = 